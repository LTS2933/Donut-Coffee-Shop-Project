<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/app_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/app_icon_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Project5cs213"
        tools:targetApi="31">
        <activity
            android:name="com.example.project5cs213.CoffeeActivity"
            android:exported="false"
            android:label="@string/title_activity_coffee"
            android:theme="@style/Theme.Project5cs213" />
        <activity
            android:name="com.example.project5cs213.BasketActivity"
            android:exported="false"
            android:label="@string/title_activity_basket"
            android:theme="@style/Theme.Project5cs213" />
        <activity
            android:name="com.example.project5cs213.DonutActivity"
            android:exported="false"
            android:label="@string/title_activity_donut"
            android:theme="@style/Theme.Project5cs213" />
        <activity
            android:name="com.example.project5cs213.OrdersActivity"
            android:exported="false"
            android:label="@string/title_activity_orders"
            android:theme="@style/Theme.Project5cs213" />
        <activity
            android:name="com.example.project5cs213.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>